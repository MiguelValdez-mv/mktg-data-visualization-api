{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "eslint-config-prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  },
  "rules": {
    "import/prefer-default-export": "off"
  }
}
